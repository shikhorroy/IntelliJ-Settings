<templateSet group="user">
  <template name="readInteger" value="int $VAR$ = reader.readInteger();" description="reading an integer value" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="readlong" value="long $VAR$ = reader.readLong();" description="reading a long value" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="readdouble" value="double $VAR$ = reader.readDouble();" description="reading a double value" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="readcharacter" value="char $VAR$ = reader.readCharacter();" description="reading a character value" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="readstring" value="String $VAR$ = reader.readString();" description="reading a string value" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="readline" value="String $VAR$ = reader.readLine();" description="reading a line as string" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="print" value="writer.print($OBJECTS$);" description="print object(s)" toReformat="true" toShortenFQNames="true">
    <variable name="OBJECTS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="printline" value="writer.printLine($OBJECTS$);" description="print object(s) with newline at last" toReformat="true" toShortenFQNames="true">
    <variable name="OBJECTS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="2^nCases" value="int N = $VALUE$;//-&gt; value &lt;= 11&#10;for (int seq = 0; seq &lt; (1 &lt;&lt; N); seq++) {&#10;    for (int pos = 0; pos &lt; N; pos++) {&#10;        if ((seq &amp; (1 &lt;&lt; pos)) &gt; 0) {&#10;            $END$&#10;        }&#10;    }&#10;}" description="Generates all possible cases with access index" toReformat="true" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>